{
  "configurations": {
    "<name>: Launch": {
      "adapter": "debugpy",
      "filetypes": [ "python" ],
      "configuration": {
        "name": "launch file",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "python": "python3",
        "stopOnEntry": false,
        "console": "externalTerminal",
        "debugOptions": [],
        "program": "${file}"
      }
    }
  }
}
