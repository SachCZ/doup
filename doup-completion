#/usr/bin/env bash
_doup()
{
    if [ "${#COMP_WORDS[@]}" != "2" ]; then
        return
    fi
    local replies=()
    for entry in "${DOUP_SEARCHDIR}/"*; do
        if [ -d $entry ]; then
            replies+=($( basename $entry))
        fi
    done
    COMPREPLY=($(compgen -W "${replies[*]}" "${COMP_WORDS[1]}"))
}

complete -F _doup doup
